{
  "setupFiles": [
    "<rootDir>/src/setupTests.js"
  ],
  "testRegex": "/*.test.js$",
  "collectCoverage": true,
  "coverageDirectory": "test-coverage",
  "coverageThreshold": {
    "global": {
      "branches": 0,
      "functions": 0,
      "lines": 0,
      "statements": 0
    }
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "snapshotSerializers": [
    "enzyme-to-json/serializer"
  ],
  "collectCoverageFrom": [
    "src/**/*.js",
    "!src/index.js"
  ]
}